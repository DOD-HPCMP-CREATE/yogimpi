<MpichAPI>
  <Function name="MPI_Keyval_create">
    <ReturnType>int</ReturnType>
    <Arg input="true" name="copy_fn" type="MPI_Copy_function*">
      <Convert>MPI_NULL_COPY_FN</Convert>
      <Convert>MPI_DUP_FN</Convert>
    </Arg>
    <Arg input="true" name="delete_fn" type="MPI_Delete_function*">
      <Convert>MPI_NULL_DELETE_FN</Convert>
    </Arg>
    <Arg output="true" name="keyval" type="int*" />
    <Arg input="true" name="extra_state" type="void*" />
  </Function>
  <Function name="MPI_Comm_create_errhandler">
    <ReturnType>int</ReturnType>
    <Arg name="function" type="MPI_Comm_errhandler_function*" />
    <Arg output="true" name="errhandler" type="MPI_Errhandler*" />
  </Function>
  <Function name="MPI_Op_create">
    <ReturnType>int</ReturnType>
    <Arg input="true" name="function" type="MPI_User_function*" />
    <Arg input="true" name="commute" type="int" />
    <Arg output="true" name="op" type="MPI_Op*" />
  </Function>
</MpichAPI>